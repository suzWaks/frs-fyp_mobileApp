{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": [
      "nativewind/types"
    ],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "app/**/*.js",
    "app/**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
