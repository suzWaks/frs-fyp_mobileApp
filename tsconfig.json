{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["nativewind/types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/modules.jsx", "app/index.jsx", "app/_layout.jsx", "app/(auth)/signin.jsx"  ]
}
